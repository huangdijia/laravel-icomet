{
    "name": "huangdijia/laravel-icomet",
    "description": "IComet client for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "icomet"
    ],
    "homepage": "https://github.com/huangdijia/laravel-icomet",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/console": "^5.7|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Huangdijia\\IComet\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\IComet\\ICometServiceProvider"
            ],
            "aliases": {
                "IComet": "Huangdijia\\IComet\\Facades\\IComet"
            }
        }
    }
}